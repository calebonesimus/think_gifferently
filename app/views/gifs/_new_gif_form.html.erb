<%= bootstrap_form_for @gif, remote: true do |f| %>

  <div class="errorsContainer">
  </div>


  <div class="field">
    <%= f.text_field :title %>
  </div>

  <div class="field">
    <%= f.text_field :gif_url %>
  </div>

  <div class="field">
    <%= f.file_field :local_gif %>
  </div>

  <div class="field">
    <%= f.text_field :tag_list, value: @gif.tag_list.to_s %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
